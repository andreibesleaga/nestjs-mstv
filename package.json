{
  "name": "nestjs-template-variant",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:dev": "ts-node -r tsconfig-paths/register src/apps/api-gateway/src/main.ts",
    "build": "tsc -p tsconfig.json",
    "test": "jest --runInBand",
    "test:unit": "jest --config jest.unit.config.js",
    "test:integration": "jest --config jest.integration.config.js",
    "test:e2e": "jest --config jest.e2e.config.js",
    "test:e2e:full": "jest --config jest.e2e.full.config.js",
    "test:all": "pnpm test:unit && pnpm test:integration && pnpm test:e2e",
    "lint": "eslint '{src,test}/**/*.{js,ts}' --fix",
    "format": "prettier --write '{src,test}/**/*.{js,ts,json}'",
    "prepare": "husky",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev --name init --preview-feature",
    "prisma:seed": "ts-node prisma/seed.ts"
  },
  "lint-staged": {
    "*.{ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js}": [
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "workspaces": [
    "apps/*",
    "packages/*",
    "modules/*"
  ],
  "devDependencies": {
    "@fastify/static": "^6.12.0",
    "@fastify/swagger": "^8.12.0",
    "@fastify/swagger-ui": "^2.1.0",
    "@nestjs/swagger": "^8.1.1",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^20.0.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^29.0.0",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.4.1",
    "ts-morph": "^26.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@apollo/gateway": "^2.0.0",
    "@apollo/server": "^4.12.2",
    "@as-integrations/fastify": "^2.1.1",
    "@casl/ability": "^6.7.0",
    "@fastify/cors": "^9.0.1",
    "@nestjs/apollo": "^12.2.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/graphql": "^12.2.2",
    "@nestjs/platform-fastify": "^10.0.0",
    "@prisma/client": "^5.0.0",
    "bcrypt": "^6.0.0",
    "bullmq": "^4.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "fastify": "^4.24.3",
    "graphql": "^16.11.0",
    "graphql-tools": "^9.0.20",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.0",
    "kafkajs": "^2.2.4",
    "pino": "^8.0.0",
    "prisma": "^5.0.0",
    "reflect-metadata": "^0.1.13"
  }
}
