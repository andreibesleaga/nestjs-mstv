{
  "name": "nestjs-template-variant",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:dev": "ts-node -r tsconfig-paths/register apps/api-gateway/src/main.ts",
    "build": "tsc -p tsconfig.json",
    "test": "jest --runInBand",
    "test:unit": "jest --config jest.unit.config.js",
    "test:integration": "jest --config jest.integration.config.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev --name init --preview-feature",
    "prisma:seed": "ts-node prisma/seed.ts"
  },
  "workspaces": [
    "apps/*",
    "packages/*",
    "modules/*"
  ],
  "devDependencies": {
    "@nestjs/swagger": "^8.1.1",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^20.0.0",
    "@types/supertest": "^2.0.12",
    "jest": "^29.0.0",
    "supertest": "^6.3.3",
    "swagger-ui-express": "^5.0.1",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@apollo/gateway": "^2.0.0",
    "@apollo/server": "^4.12.2",
    "@casl/ability": "^6.7.0",
    "@nestjs/apollo": "^12.2.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/graphql": "^12.2.2",
    "@prisma/client": "^5.0.0",
    "bcrypt": "^5.1.0",
    "bullmq": "^4.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "express": "^4.18.2",
    "graphql": "^16.11.0",
    "graphql-tools": "^9.0.20",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.0",
    "kafkajs": "^2.2.4",
    "pino": "^8.0.0",
    "prisma": "^5.0.0",
    "reflect-metadata": "^0.1.13",
    "ts-morph": "^17.0.1"
  }
}
